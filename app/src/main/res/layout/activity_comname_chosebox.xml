<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Yinhuan_tv_comname_r1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/Yinhuan_tv_comname_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:gravity="center"
        />

    <CheckBox
        android:id="@+id/Yinhuan_cb_comname_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Yinhuan_tv_comname_item"
        android:layout_alignBottom="@+id/Yinhuan_tv_comname_item"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false" />



</RelativeLayout>